{
  "compilerOptions": {
    "strict": true,
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "newLine": "LF",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": false,
    "baseUrl": ".",
    "outDir": "./lib",
    "paths": {
      "antd-x-form": ["./src/index.ts"]
    },
    "lib": ["esnext", "DOM"]
  },
  "include": ["src/**/*"],
  "exclude": ["**/*.test.ts", "**/__*__"]
}
